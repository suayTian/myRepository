<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tian.springcloud.mapper.CompanyMapper">

    <insert id="insert" parameterType="com.tian.springcloud.pojo.Company">
        INSERT INTO `springtest`.`company`(`org_code`, `org_name`)
        VALUES (#{orgCode}, #{orgName});
    </insert>

    <select id="getList" resultType="com.tian.springcloud.pojo.Company">
        SELECT *
        FROM company
    </select>

    <select id="selectById" resultType="com.tian.springcloud.pojo.Company">
        SELECT *
        FROM company
        WHERE id = #{id}
    </select>

</mapper>